{
  "version": 1,
  "types": {
    "FloatEqualsValidation": {
      "fields": {
        "pi": {
          "type": "float",
          "rules": {
            "equals": {
              "value": 3.14159,
              "errorMessage": "Value must be equal to PI (3.14159)"
            }
          }
        }
      }
    }
  },
  "procedures": {
    "ValidatePI": {
      "input": {
        "data": {
          "type": "FloatEqualsValidation"
        }
      },
      "output": {
        "isValid": {
          "type": "boolean"
        }
      }
    }
  }
}

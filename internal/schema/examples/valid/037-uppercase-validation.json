{
  "version": 1,
  "types": {
    "UppercaseValidation": {
      "type": "object",
      "fields": {
        "countryCode": {
          "type": "string",
          "rules": {
            "uppercase": {
              "errorMessage": "Country code must be in uppercase"
            }
          }
        }
      }
    }
  },
  "procedures": {
    "ValidateCountryCode": {
      "type": "query",
      "input": {
        "type": "UppercaseValidation"
      },
      "output": {
        "type": "boolean"
      }
    }
  }
}

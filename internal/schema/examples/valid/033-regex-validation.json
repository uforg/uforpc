{
  "version": 1,
  "types": {
    "RegexValidation": {
      "type": "object",
      "fields": {
        "zipCode": {
          "type": "string",
          "rules": {
            "regex": {
              "value": "^\\d{5}(-\\d{4})?$",
              "errorMessage": "Must be a valid US zip code"
            }
          }
        }
      }
    }
  },
  "procedures": {
    "ValidateZipCode": {
      "type": "query",
      "input": {
        "type": "RegexValidation"
      },
      "output": {
        "type": "boolean"
      }
    }
  }
}

{
  "version": 1,
  "types": {
    "IntEnumValidation": {
      "type": "object",
      "fields": {
        "httpStatus": {
          "type": "int",
          "rules": {
            "enum": {
              "values": [200, 201, 400, 401, 403, 404, 500],
              "errorMessage": "HTTP status must be a standard code"
            }
          }
        }
      }
    }
  },
  "procedures": {
    "ValidateHttpStatus": {
      "type": "query",
      "input": {
        "type": "IntEnumValidation"
      },
      "output": {
        "type": "boolean"
      }
    }
  }
}

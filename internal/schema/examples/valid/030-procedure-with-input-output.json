{
  "version": 1,
  "types": {
    "UserInput": {
      "type": "object",
      "fields": {
        "username": {
          "type": "string",
          "rules": {
            "minLen": {
              "value": 3,
              "errorMessage": "Username must be at least 3 characters"
            }
          }
        },
        "password": {
          "type": "string",
          "rules": {
            "minLen": {
              "value": 8,
              "errorMessage": "Password must be at least 8 characters"
            }
          }
        }
      }
    },
    "AuthResponse": {
      "type": "object",
      "fields": {
        "token": {
          "type": "string"
        },
        "expiresAt": {
          "type": "string",
          "rules": {
            "iso8601": {
              "errorMessage": "Must be a valid ISO8601 date"
            }
          }
        }
      }
    }
  },
  "procedures": {
    "Login": {
      
      "description": "Authenticates a user and returns a token",
      "input": {
        "type": "UserInput"
      },
      "output": {
        "type": "AuthResponse"
      }
    }
  }
}
